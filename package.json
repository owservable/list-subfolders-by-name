{
	"name": "@owservable/list-subfolders-by-name",
	"version": "0.0.1",
	"description": "list subfolders by name",
	"engines": {
		"node": ">=16"
	},
	"main": "./lib/list.subfolders.by.name.js",
	"types": "./lib/list.subfolders.by.name.d.ts",
	"author": "Predrag Stojadinovic <predrag@stojadinovic.net>",
	"license": "Unlicense",
	"files": [
		"lib/**/*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/owservable/list-subfolders-by-name.git"
	},
	"scripts": {
		"all": "yarn & tsc & yarn clean & yarn build & yarn test & yarn test_qube & yarn docs",
		"build": "del /S /Q lib\\* && tsc",
		"build:watch": "tsc --watch",
		"lint": "tslint -p tsconfig.json",
		"clean": "tslint --config tslint-imports.json --fix --project .",
		"format": "prettier --write \"src/**/*.ts\"",
		"docs": "typedoc --readme none --out docs ./src/list.subfolders.by.name.ts",
		"test": "nyc --all --reporter=lcov --reporter=html mocha -r ts-node/register test/**/*.spec.ts --code-coverage",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"sonar": "sonarjs -e .sonarlint/**,node_modules/**,coverage/**,.nyc_output/**",
		"test_qube": "nyc --reporter=lcov mocha -R @danmasta/mocha-sonar -r ts-node/register test/**/*.spec.ts",
		"up": "yarn upgrade-interactive",
		"latest": "yarn upgrade-interactive --latest",
		"srctree": "dir src /s /b > srctree.txt"
	},
	"dependencies": {
		"@types/lodash": "^4.14.182",
		"@types/node": "^17.0.31",
		"lodash": "^4.17.20"
	},
	"devDependencies": {
		"@danmasta/mocha-sonar": "^1.0.1",
		"@types/chai": "^4.3.1",
		"@types/mocha": "^9.1.1",
		"chai": "^4.3.6",
		"coveralls": "^3.0.9",
		"mocha": "^10.0.0",
		"nyc": "^15.1.0",
		"prettier": "^2.6.2",
		"sonarjs": "^1.0.0",
		"source-map-support": "^0.5.21",
		"ts-node": "^10.7.0",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"tslint-etc": "^1.13.7",
		"tslint-no-unused-expression-chai": "^0.1.4",
		"typedoc": "^0.22.15",
		"typescript": "^4.6.4"
	}
}
